<p style="color: green"><%= notice %></p>
<%= render @profile %>
<div class="container container--constrain">
  <div class="links">
    <%= link_to "Edit this profile", edit_profile_path(@profile) %>
    <%= link_to "Back to profiles", profiles_path %>
    <%= button_to "Destroy this profile", @profile, method: :delete, class: "button button--small" %>
  </div>
</div>
