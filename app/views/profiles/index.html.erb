<header class="header">
  <h1>Profiles</h1>
  <%= render 'feedback' %>
</header>
<div class="container container--constrain">
  <div class="links">
    <%= link_to "New profile", new_profile_path %>
  </div>
</div>
<div class="container container--constrain">
  <div class="profiles">
    <% @profiles.each do |profile| %>
      <%= render profile %>
    <% end %>
  </div>
</div>
<%= render 'admin/links' %>
